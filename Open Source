    local Teleport = Window:NewTab("Teleport")
    local TeleportSection = Teleport:NewSection("Teleport")

    
    local Teleporter; 
    TeleportSection:NewDropdown("Select Island", "Click the number you want (Refers to how far down it is)", {"Jungle", "Aqua", "Farm", "Magic", "Arcade",}, function(value)--THIS FUNCTION ALLOWS US TO STORE THE USERS SELECTION
        Teleporter = value; 
    end)

    TeleportSection:NewButton("Teleport", "Teleports", function(v)
    if Teleporter == "Jungle" then  
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(362.041565, 302.327698, 1474.49939, 0.999814451, -2.06811936e-08, 0.0192637611, 2.09630038e-08, 1, -1.44270835e-08, -0.0192637611, 1.48282329e-08, 0.999814451)


    else if Teleporter == "Aqua" then

        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(373.308136, 301.746643, 1217.95312, 0.997130454, 1.78758093e-08, -0.0757020563, -1.7733564e-08, 1, 2.55120836e-09, 0.0757020563, -1.20142041e-09, 0.997130454)

    else if Teleporter == "Farm" then

        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(368.242249, 294.114349, 1951.74902, 0.999239504, 4.96408106e-08, 0.0389928631, -4.66417909e-08, 1, -7.782171e-08, -0.0389928631, 7.59438308e-08, 0.999239504)

        else if Teleporter == "Magic" then

            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(440.079346, 290.730133, 2551.35889, 0.998676538, 0, 0.0514308959, 0, 1, 0, -0.0514308959, 0, 0.998676538)

            else if Teleporter == "Arcade" then

                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(84.4777985, 304.215515, 1588.76501, 0.720068932, -0.00809190702, 0.693855405, -0.00857219193, 0.999751985, 0.0205553956, -0.693849683, -0.0207491629, 0.719820976)

            end
        end
    end
end
end
