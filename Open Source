local Eggs = Window:NewTab("Eggs")
local EggsSection = Eggs:NewSection("Eggs")

local Egg; 
    EggsSection:NewDropdown("Select Egg", "Selects Egg", {"First Egg (Spawn)", "Second Egg (Spawn)", "Third Egg (Jungle)", "Forth Egg (Aqua)", "Fifth Egg (Farm)", "Sixth Egg (Magic)", "Seventh Egg (Arcade)", "500K Egg (Event)",}, function(value)
        Egg = value; 
    end)


    EggsSection:NewToggle("Auto Open", "Auto Opens Eggs", function(v)

        getgenv().autoclickers = v
        while true do
            if not getgenv().autoclickers then return end
wait(1)


if Egg == "First Egg (Spawn)" then
    local args = {
        [1] = 1,
        [2] = 1,
        [3] = workspace.Eggs["0"]["1"]
    }
    
    game:GetService("ReplicatedStorage").Remotes.BuyEgg:FireServer(unpack(args))
    
    wait(1)
else if Egg == "Second Egg (Spawn)" then
    local args = {
        [1] = 2,
        [2] = 1,
        [3] = workspace.Eggs["0"]["2"]
    }
    
    game:GetService("ReplicatedStorage").Remotes.BuyEgg:FireServer(unpack(args))
    
        wait(1)
    else if Egg == "Third Egg (Jungle)" then
        local args = {
