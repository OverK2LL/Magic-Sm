if game.PlaceId == 9170761016 then
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("Magic Clicker Simulator", "BloodTheme")

    local Main = Window:NewTab("Main")
    local MainSection = Main:NewSection("Main")



    MainSection:NewToggle("Auto Tap", "Taps", function(v)
        getgenv().autoswing = v
        while true do
            if not getgenv().autoswing then return end

            local Event = game:GetService("ReplicatedStorage").Remotes.CheckClick
            Event:FireServer()
            
wait()

        end
    end)

    MainSection:NewToggle("Auto Craft", "Crafts", function(v)
        getgenv().autoswings = v
        while true do
            if not getgenv().autoswings then return end

            local A_1 = true
            local Event = game:GetService("ReplicatedStorage").Remotes.CraftAllPets
            Event:FireServer(A_1)

wait()

        end
    end)


    MainSection:NewToggle("Auto Equip Best Pet", "Jajajaja", function(v)
        getgenv().autoswingss = v
        while true do
            if not getgenv().autoswingss then return end

            game:GetService("ReplicatedStorage").Remotes.EquipBest:FireServer()
wait(5)

        end
    end)

    local Rebirths; 
